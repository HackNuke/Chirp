{
    "editor.renderWhitespace": "all",
    "editor.formatOnSave": true,
    "editor.rulers": [
        80,
        120
    ],
    "editor.quickSuggestionsDelay": 100,
    "search.quickOpen.includeSymbols": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "python.linting.prospectorEnabled": true,
    "python.linting.flake8Enabled": false,
    "python.linting.pep8Enabled": true,
    "python.linting.pylintPath": "/path/to/virtualenv/bin/pylint",
    "python.linting.pylintArgs": [
        "--load-plugins",
        "pylint_django"
    ],
    "python.linting.pylintEnabled": true,
    "python.linting.pydocstyleEnabled": true,
    "python.linting.maxNumberOfProblems": 500,
    "python.unitTest.pyTestEnabled": false,
    "python.unitTest.unittestEnabled": true,
    "python.unitTest.nosetestsEnabled": false,
    "workbench.colorTheme": "Spacemacs - dark",
    "python.linting.mypyEnabled": false,
    "python.venvPath": "$(workspaceroot)/venv",
    "python.pythonPath": "${workspaceRoot}/venv/bin/python3",
    "git.ignoreLimitWarning": true,
}